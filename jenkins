pipeline {
    agent any

    stages {
        stage('clone') {
            steps {
                git branch: 'main', credentialsId: '10667098-1d0b-47d7-a51e-08930d9be970', url: 'https://github.com/PixelPawar/HelloWorld.git'
            }
        }
        stage('Compile') {
            steps {
                bat 'javac helloworld.java'
            }
        }
        stage('Run') {
            steps {
                bat 'java helloworld'
            }
        }
    }
}
