pipeline {
    agent any

    stages {
        stage('clone') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Compile') {
            steps {
                bat 'javac helloworld.java'
            }
        }
        stage('Run') {
            steps {
                bat 'java helloworld'
            }
        }
    }
}
